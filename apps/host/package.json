{
  "name": "@mobileworship/host",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "dotenv -- react-native run-android",
    "android:release": "dotenv -- bash -c 'cd android && ./gradlew assembleRelease'",
    "android:install": "adb install android/app/build/outputs/apk/release/app-release.apk",
    "ios": "dotenv -- react-native run-ios",
    "macos": "dotenv -- react-native run-macos",
    "windows": "dotenv -- react-native run-windows",
    "start": "dotenv -- react-native start",
    "start:reset": "dotenv -- react-native start --reset-cache",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@supabase/supabase-js": "^2.43.0",
    "@tanstack/react-query": "^5.32.0",
    "nativewind": "^2.0.0",
    "react": "18.2.0",
    "react-native": "0.74.0",
    "react-native-keep-awake": "^4.0.0",
    "react-native-network-info": "^5.2.2",
    "react-native-qrcode-svg": "^6.3.21",
    "react-native-svg": "^15.2.0",
    "react-native-tcp-socket": "^6.3.0",
    "react-native-url-polyfill": "^3.0.0",
    "react-native-video": "^6.1.0",
    "react-native-zeroconf": "^0.13.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/preset-env": "^7.24.0",
    "@babel/runtime": "^7.24.0",
    "@react-native-community/cli": "^13.6.4",
    "@react-native-community/cli-platform-android": "^13.6.4",
    "@react-native/babel-preset": "0.74.0",
    "@react-native/eslint-config": "0.74.0",
    "@react-native/gradle-plugin": "^0.74.0",
    "@react-native/metro-config": "0.74.0",
    "@react-native/typescript-config": "0.74.0",
    "@types/react": "^18.3.0",
    "babel-plugin-transform-inline-environment-variables": "^0.4.4",
    "dotenv-cli": "^7.4.2",
    "tailwindcss": "3.3.2",
    "typescript": "^5.4.0"
  }
}
